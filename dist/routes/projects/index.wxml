<block>
    <view class="projects">
        <card padding="{{true}}" __triggerObserer="{{ _triggerObserer }}">
            <block>
                <block wx:if="{{loading}}">
                    <loading __triggerObserer="{{ _triggerObserer }}"></loading>
                </block>
                <block wx:else>
                    <view class="project" wx:key="{{i}}" wx:for="{{main}}" wx:for-item="item"
                    wx:for-index="i">
                        <project data="{{item}}" __triggerObserer="{{ _triggerObserer }}"></project>
                    </view>
                </block>
            </block>
        </card>
        <white-space __triggerObserer="{{ _triggerObserer }}"></white-space>
        <card title="School Works" __triggerObserer="{{ _triggerObserer }}"
        padding="{{true}}">
            <block>
                <block wx:if="{{loading}}">
                    <loading __triggerObserer="{{ _triggerObserer }}"></loading>
                </block>
                <block wx:else>
                    <view class="project" wx:key="{{i}}" wx:for="{{extra}}" wx:for-item="item"
                    wx:for-index="i">
                        <project data="{{item}}" __triggerObserer="{{ _triggerObserer }}"></project>
                    </view>
                </block>
            </block>
        </card>
        <white-space __triggerObserer="{{ _triggerObserer }}"></white-space>
        <tabbar-shadow __triggerObserer="{{ _triggerObserer }}"></tabbar-shadow>
    </view>
</block>